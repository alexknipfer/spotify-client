{
  "name": "spotify-client-server",
  "version": "0.0.1",
  "author": "Alex Knipfer <alexanderknipfer@gmail.com>",
  "description": "Apollo GraphQL server for spotify client",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexknipfer/spotify-client-server.git"
  },
  "bugs": {
    "url": "https://github.com/alexknipfer/spotify-client-server/issues"
  },
  "keywords": [
    "apollo",
    "graphql",
    "typescript",
    "api"
  ],
  "scripts": {
    "dev": "nodemon --require dotenv/config ./src/main.ts",
    "start": "node --require dotenv/config dist/main",
    "build": "tsc",
    "format": "prettier --write \"**/*.{tsx,ts,css,scss,md,js,json,html}\""
  },
  "devDependencies": {
    "@types/graphql": "^14.2.0",
    "dotenv": "^7.0.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.1"
  },
  "dependencies": {
    "apollo-server-express": "^2.4.8",
    "express": "^4.16.4",
    "graphql": "^14.2.0",
    "graphql-tools": "^4.0.4"
  }
}
